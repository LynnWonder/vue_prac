{
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@": ["src/index.js", "src/index.json", "src/index.vue", "src/index.less", "src/index.css"],
      "@src/*": ["src/*"],
      "@src": ["src/index.js", "src/index.json", "src/index.vue", "src/index.less", "src/index.css"],
      "@router/*": ["src/router/*"],
      "@router": [
        "src/router/index.js",
        "src/router/index.json",
        "src/router/index.vue",
        "src/router/index.less",
        "src/router/index.css"
      ],
      "@views/*": ["src/router/views/*"],
      "@views": [
        "src/router/views/index.js",
        "src/router/views/index.json",
        "src/router/views/index.vue",
        "src/router/views/index.less",
        "src/router/views/index.css"
      ],
      "@layouts/*": ["src/router/layouts/*"],
      "@layouts": [
        "src/router/layouts/index.js",
        "src/router/layouts/index.json",
        "src/router/layouts/index.vue",
        "src/router/layouts/index.less",
        "src/router/layouts/index.css"
      ],
      "@components/*": ["src/components/*"],
      "@components": [
        "src/components/index.js",
        "src/components/index.json",
        "src/components/index.vue",
        "src/components/index.less",
        "src/components/index.css"
      ],
      "@assets/*": ["src/assets/*"],
      "@assets": [
        "src/assets/index.js",
        "src/assets/index.json",
        "src/assets/index.vue",
        "src/assets/index.less",
        "src/assets/index.css"
      ],
      "@utils/*": ["src/utils/*"],
      "@utils": [
        "src/utils/index.js",
        "src/utils/index.json",
        "src/utils/index.vue",
        "src/utils/index.less",
        "src/utils/index.css"
      ],
      "@state/*": ["src/state/*"],
      "@state": [
        "src/state/index.js",
        "src/state/index.json",
        "src/state/index.vue",
        "src/state/index.less",
        "src/state/index.css"
      ],
      "@theme/*": ["src/design/*"],
      "@theme": [
        "src/design/index.js",
        "src/design/index.json",
        "src/design/index.vue",
        "src/design/index.less",
        "src/design/index.css"
      ],
      "@services/*": ["src/services/*"],
      "@services": [
        "src/services/index.js",
        "src/services/index.json",
        "src/services/index.vue",
        "src/services/index.less",
        "src/services/index.css"
      ]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
